{"ast":null,"code":"import UpdateUsernameForm from \"../components/UpdateUsernameForm.vue\";\nimport FollowUser from \"../components/FollowUser.vue\";\nimport UnFollowUser from \"../components/UnFollowUser.vue\";\nimport BanUser from \"../components/BanUser.vue\";\nimport UnBanUser from \"../components/UnBanUser.vue\";\nimport UploadPhotoForm from \"../components/UploadPhotoForm.vue\";\nexport default {\n  name: \"UserHeader\",\n  components: {\n    UpdateUsernameForm,\n    FollowUser,\n    UnFollowUser,\n    BanUser,\n    UnBanUser,\n    UploadPhotoForm\n  },\n  data() {\n    return {\n      activeDropdown: null\n    };\n  },\n  methods: {\n    toggleDropdown(name) {\n      // Toggle visibility of the dropdown content\n      this.activeDropdown = this.activeDropdown === name ? null : name;\n    }\n  }\n};","map":{"version":3,"names":["UpdateUsernameForm","FollowUser","UnFollowUser","BanUser","UnBanUser","UploadPhotoForm","name","components","data","activeDropdown","methods","toggleDropdown"],"sources":["/Users/shivamkumar/Desktop/wasaphoto/webui/src/components/UserHeader.vue"],"sourcesContent":["\n<template>\n    <div class=\"nav\">\n      <div class=\"dropdown\">\n        <a @click=\"toggleDropdown('updateUsername')\">Update Username</a>\n        <div v-show=\"activeDropdown === 'updateUsername'\" class=\"dropdown-content\">\n          <UpdateUsernameForm />\n        </div>\n      </div>\n  \n      <div class=\"dropdown\">\n        <a @click=\"toggleDropdown('follow')\">Follow</a>\n        <div v-show=\"activeDropdown === 'follow'\" class=\"dropdown-content\">\n          <FollowUser />\n        </div>\n      </div>\n  \n      <div class=\"dropdown\">\n        <a @click=\"toggleDropdown('unfollow')\">Unfollow</a>\n        <div v-show=\"activeDropdown === 'unfollow'\" class=\"dropdown-content\">\n          <UnFollowUser />\n        </div>\n      </div>\n  \n      <div class=\"dropdown\">\n        <a @click=\"toggleDropdown('ban')\">Ban</a>\n        <div v-show=\"activeDropdown === 'ban'\" class=\"dropdown-content\">\n          <BanUser />\n        </div>\n      </div>\n      <div class=\"dropdown\">\n        <a @click=\"toggleDropdown('unban')\">UnBan</a>\n        <div v-show=\"activeDropdown === 'unban'\" class=\"dropdown-content\">\n          <UnBanUser />\n        </div>\n      </div>\n      <div class=\"dropdown\">\n        <a @click=\"toggleDropdown('upload')\">Upload Photo</a>\n        <div v-show=\"activeDropdown === 'upload'\" class=\"dropdown-content\">\n          <UploadPhotoForm />\n        </div>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import UpdateUsernameForm from \"../components/UpdateUsernameForm.vue\";\n  import FollowUser from \"../components/FollowUser.vue\";\n  import UnFollowUser from \"../components/UnFollowUser.vue\";\n  import BanUser from \"../components/BanUser.vue\";\n  import UnBanUser from \"../components/UnBanUser.vue\";\n  import UploadPhotoForm from \"../components/UploadPhotoForm.vue\";\n  \n  export default {\n    name: \"UserHeader\",\n    components: {\n      UpdateUsernameForm,\n      FollowUser,\n      UnFollowUser,\n      BanUser,\n      UnBanUser,\n      UploadPhotoForm\n    },\n    data() {\n      return {\n        activeDropdown: null,\n      };\n    },\n    methods: {\n      toggleDropdown(name) {\n        // Toggle visibility of the dropdown content\n        this.activeDropdown = this.activeDropdown === name ? null : name;\n      },\n    },\n  };\n  </script>\n\n\n<style>\n  /* Common styles for the nav bar */\n  .nav {\n    background-color: #1976D2; /* Blue color, you can change it based on your preference */\n    overflow: hidden;\n    margin-top: 0;\n    height: 50px;\n    font-size: large;\n    display: flex;\n    align-items: center;\n    justify-content: flex-end; /* Adjust the alignment based on your design */\n  }\n\n  .dropdown {\n    position: relative;\n    margin-right: 20px; /* Adjust the spacing between dropdowns */\n  }\n\n  .dropdown-content {\n    display: none;\n    position: absolute;\n    background-color: #fff; /* White background for the dropdown */\n    min-width: 160px;\n    box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);\n    z-index: 1;\n    border-radius: 8px; /* Add some rounded corners */\n    padding: 10px; /* Adjust the padding */\n  }\n\n  .dropdown:hover .dropdown-content {\n    display: block;\n  }\n\n  .nav a {\n    color: white; /* White text color */\n    padding: 14px 16px;\n    text-align: center;\n    font-size: 17px;\n    text-decoration: none;\n    margin-right: 5px;\n    cursor: pointer;\n    transition: background-color 0.3s ease; /* Smooth transition for the background */\n    border-radius: 8px; /* Add some rounded corners */\n  }\n\n  .nav a:hover {\n    background-color: #135592; /* Darker blue color on hover, adjust based on your preference */\n  }\n</style>\n  \n  <!-- <style scoped>\n  .nav {\n    background-color: aqua;\n    overflow: hidden;\n    margin-top: 0%;\n    height: 50px;\n    font-size: large;\n  }\n  \n  .dropdown {\n    float: left;\n  }\n  \n  .dropdown-content {\n    display: none;\n    position: absolute;\n    background-color: #f9f9f9;\n    min-width: 160px;\n    box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);\n    z-index: 1;\n  }\n  \n  .dropdown:hover .dropdown-content {\n    display: block;\n  }\n  \n  .nav a {\n    color: black;\n    padding: 14px 16px;\n    text-align: center;\n    font-size: 17px;\n    text-decoration: none;\n    margin-right: 5px;\n    cursor: pointer;\n  }\n  </style>\n   -->"],"mappings":"AA8CE,OAAOA,kBAAiB,MAAO,sCAAsC;AACrE,OAAOC,UAAS,MAAO,8BAA8B;AACrD,OAAOC,YAAW,MAAO,gCAAgC;AACzD,OAAOC,OAAM,MAAO,2BAA2B;AAC/C,OAAOC,SAAQ,MAAO,6BAA6B;AACnD,OAAOC,eAAc,MAAO,mCAAmC;AAE/D,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVP,kBAAkB;IAClBC,UAAU;IACVC,YAAY;IACZC,OAAO;IACPC,SAAS;IACTC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACL,IAAI,EAAE;MACnB;MACA,IAAI,CAACG,cAAa,GAAI,IAAI,CAACA,