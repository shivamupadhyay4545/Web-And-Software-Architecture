<div v-show=\"activeDropdown === 'unfollow'\">\n          <UnFollowUser />\n        </div>\n      </div>\n  \n      <div>\n        <a @click=\"toggleDropdown('ban')\">Ban</a>\n        <div v-show=\"activeDropdown === 'ban'\">\n          <BanUser />\n        </div>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import UpdateUsernameForm from \"./components/UpdateUsernameForm.vue\";\n  import FollowUser from \"./components/FollowUser.vue\";\n  import UnFollowUser from \"./components/UnFollowUser.vue\";\n  import BanUser from \"./components/BanUser.vue\";\n  \n  export default {\n    name: \"UserHeader\",\n    components: {\n      UpdateUsernameForm,\n      FollowUser,\n      UnFollowUser,\n      BanUser,\n    },\n    data() {\n      return {\n        activeDropdown: null,\n      };\n    },\n    methods: {\n      toggleDropdown(name) {\n        // Toggle visibility of the dropdown content\n        this.activeDropdown = this.activeDropdown === name ? null : name;\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .nav {\n    background-color: aqua;\n    overflow: hidden;\n  }\n  \n  .nav div {\n    float: left;\n  }\n  \n  .nav a {\n    color: black;\n    padding: 14px 16px;\n    text-align: center;\n    font-size: 17px;\n    text-decoration: none;\n    margin-right: 5px;\n    cursor: pointer;\n  }\n  \n  /* Style for the dropdown content */\n  .nav div > div {\n    display: none;\n    position: absolute;\n    background-color: #f9f9f9;\n    min-width: 160px;\n    box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);\n    z-index: 1;\n  }\n  \n  .nav div:hover > div {\n    display: block;\n  }\n  </style>\n  "],"mappings":"AAkEE,OAAOA,kBAAiB,MAAO,qCAAqC;AACpE,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,OAAM,MAAO,0BAA0B;AAE9C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVL,kBAAkB;IAClBC,UAAU;IACVC,YAAY;IACZC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACL,IAAI,EAAE;MACnB;